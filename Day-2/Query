For JdbcConcurrency.java

drop table Employees;
 
create table Employees(
     id int NOT NULL PRIMARY KEY,
     age int NOT NULL,
     first varchar (255),
     last varchar (255)
     );
     
INSERT INTO Employees VALUES (100, 21, 'JACK', 'EDITION');
